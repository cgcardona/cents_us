class Census
  constructor : (@authKey) ->

  queryCensus : ->
    $.get 'http://api.census.gov/data/2010/sf1?key=' + @authKey + '&get=P0050013,NAME&for=state:*', this.callback

  callback : (data) ->
    console.log(data)
  
$(document).ready ->
  cents_us = new Census($('#authKey').attr('data-authKey'))
  cents_us.queryCensus()
